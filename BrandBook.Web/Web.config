  </configSections>
  <appSettings file="MachineAppSettings.config">
    <!-- AppVersion -->
    <add key="AppVersion" value="2.0.0" />

    <!-- Definitions -->
    <add key="EnabledAppCultures" value="en-US,de-DE" />
	<add key="NotificationSpamFilterIdentificationKeywords" value="seo,spam,improve your seo,ranking keywords,cheap rates,domain authority,generating paying customers,more leads,act now,apply now,apply online,click here,exclusive deal,get it now,important information regarding,limited time,new customers only,offer expires,order now,special promotion,apply for grant" />
	<add key="TrialSubscriptionPlanId" value="7" />
	  
    <!-- Domains / Urls -->
    <add key="AuthorMainWebsiteUrl" value="https://philipp-moser.de" />
    <add key="FacebookPageUrl" value="https://www.facebook.com/brandci.cloud" />
    <add key="GithubPageUrl" value="https://github.com/BrandCi" />
    <add key="BrandCiDocumentationUrl" value="https://documentation.brandci.cloud" />
    <!-- System -->
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <connectionStrings configSource="ConnectionStrings.config" />
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.8" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.8" />
    <httpRuntime enableVersionHeader="false" />
    <customErrors mode="RemoteOnly" defaultRedirect="~/Error/500.html">
      <error statusCode="400" redirect="~/Error/400.html" />
      <error statusCode="401" redirect="~/Error/401.html" />
      <error statusCode="403" redirect="~/Error/403.html" />
      <error statusCode="404" redirect="~/Error/404.html" />
      <error statusCode="500" redirect="~/Error/500.html" />
    </customErrors>
  </system.web>
